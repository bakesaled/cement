{
  "name": "@bakesaled/cement",
  "version": "0.0.1",
  "description": "A library that encrypts and decrypt files or strings.",
  "author": "Brandon Eleuterio",
  "license": "MIT",
  "keywords": [
    "TypeScript",
    "JavaScript",
    "Encrypt",
    "Decrypt",
    "Argon2"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/main.js",
  "module": "dist/main.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "tsc-watch --onSuccess \"node ./dist/examples/index.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:ci": "npm run test:cov -- --runInBand --ci --logHeapUsage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "argon2": "^0.25.1",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "25.1.1",
    "@types/node": "^13.1.6",
    "@types/node-gzip": "^1.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "mock-fs": "^4.10.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.31.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsc-watch": "^4.1.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
